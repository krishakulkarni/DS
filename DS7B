#include <iostream>
using namespace std;

int main() {
    int n, e;
    int graph[10][10], visited[10], stack[10];
    int top = -1;

    cout << "Enter number of web pages (max 10): ";
    cin >> n;

    // Initialize graph and visited
    for (int i = 0; i < n; i++) {
        visited[i] = 0;
        for (int j = 0; j < n; j++)
            graph[i][j] = 0;
    }

    cout << "\nEnter number of hyperlinks: ";
    cin >> e;

    cout << "\nEnter links (from to):\n";
    for (int i = 0; i < e; i++) {
        int a, b;
        cin >> a >> b;
        graph[a][b] = 1;
    }

    int start;
    cout << "\nEnter seed (start) page number: ";
    cin >> start;

    cout << "\n\nStarting DFS web crawling from page: " << start << "\n";

    // Push start page into stack
    stack[++top] = start;
    visited[start] = 1;

    while (top >= 0) {
        int page = stack[top--];   // Pop
        cout << "\n Indexed page: " << page;

        // Visit connected pages
        for (int i = 0; i < n; i++) {
            if (graph[page][i] == 1 && visited[i] == 0) {
                stack[++top] = i;   // Push
                visited[i] = 1;
            }
        }
    }

    return 0;
}

