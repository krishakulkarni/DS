#include <iostream>
using namespace std;
int main(){
	int n;
	cout<<"Enter Number of scores:"<<endl;
	cin>>n;
	int scores[n];
	cout<<"enter scores:";
	for(int i=0;i<n;i++){
		cin>>scores[i];
	}
   for(int i=0;i<n;i++){
		for(int j=0;j<n-i-1;j++){
			if(scores[j]>scores[j+1]){
				int temp=scores[j];
				scores[j]=scores[j+1];
				scores[j+1]=temp;
	            }
			}
		} 
	cout<<"Level up Soreted score:\n";
	for(int i=0;i<n;i++){
		cout<<scores[i]<<" ";
	}
	cout<<endl;
	return 0;
  }


